    private void cbboxflavourActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
        if(cbboxflavour.getSelectedIndex()==1){
            txtcpcakeflavour.setText("Chocolate");
            txtflvharga.setText("5000");
        }
        else if(cbboxflavour.getSelectedIndex()==2){
            txtcpcakeflavour.setText("Vanila");
            txtflvharga.setText("5000");
        }
        else if(cbboxflavour.getSelectedIndex()==3){
            txtcpcakeflavour.setText("Strawberry");
            txtflvharga.setText("6000");
        }
        else if(cbboxflavour.getSelectedIndex()==4){
            txtcpcakeflavour.setText("Matcha");
            txtflvharga.setText("8000");
        }
    }                                            

    private void cbboxtoppActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        if(cbboxtopp.getSelectedIndex()==1){
            txtcpcaketopp.setText("Peanut");
            txttoppharga.setText("500");
        }
        else if(cbboxtopp.getSelectedIndex()==2){
            txtcpcaketopp.setText("Springkle");
            txttoppharga.setText("500");
        }
        else if(cbboxtopp.getSelectedIndex()==3){
            txtcpcaketopp.setText("Dark Choco");
            txttoppharga.setText("1000");
        }
        else if(cbboxtopp.getSelectedIndex()==4){
            txtcpcaketopp.setText("Cheese");
            txttoppharga.setText("1000");
        }
    }                                         

    private void butthtgbayarActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
        int a, b, c;
        a = Integer.valueOf(txtflvharga.getText());
        b = Integer.valueOf(txttoppharga.getText());
        c =a+b;
        txthtgbayar.setText(""+c);
    }                                            

    private void txtjmlbayarActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        int a, b, c;
        a = Integer.valueOf(txtjmlbayar.getText());
        b = Integer.valueOf(txthtgbayar.getText());
        c = a-b;
        txtkembalian.setText(""+c);
    }                                           

    private void buttkeluarActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        dispose();
    }                                          

    private void buttprossesActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        buttprosses.getText();
        txtArea.setText(
        "Cupcake Flavour            :"+txtcpcakeflavour.getText()+"\n"+
        "Harga                      :"+txtflvharga.getText()+"\n"+
        "Topping                    :"+txtcpcaketopp.getText()+"\n"+
        "Harga                      :"+txttoppharga.getText()+"\n"+
        "Total Bayar                :"+txthtgbayar.getText()+"\n"+
        "Jumlah Bayar               :"+txtjmlbayar.getText()+"\n"+
        "Kembalian                  :"+txtkembalian.getText()+"\n");
        
    }                                           

    private void txtcpcakeflavourActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        // TODO add your handling code here:
    }                                                

    private void txtkembalianActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
    }                                            

